<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="SensorDataWebGui.presentation.SensorDataWebGuiModelWizardID"
            name="%_UI_SensorDataWebGuiModelWizard_label"
            class="SensorDataWebGui.presentation.SensorDataWebGuiModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SensorDataWebGuiModelFile.gif">
         <description>%_UI_SensorDataWebGuiModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="SensorDataWebGui.presentation.SensorDataWebGuiEditorID"
            name="%_UI_SensorDataWebGuiEditor_label"
            icon="icons/full/obj16/SensorDataWebGuiModelFile.gif"
            extensions="sdw"
            class="SensorDataWebGui.presentation.SensorDataWebGuiEditor"
            contributorClass="SensorDataWebGui.presentation.SensorDataWebGuiActionBarContributor">
      </editor>
   </extension>

</plugin>
